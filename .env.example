# =============================================================================
# CONFIGURATION ENVIRONNEMENT - 12-FACTOR APP COMPLIANT
# =============================================================================
# Copiez ce fichier vers .env et modifiez les valeurs selon votre environnement
# Ne committez JAMAIS le fichier .env avec des valeurs réelles

# -----------------------------------------------------------------------------
# MySQL Database Configuration (OBLIGATOIRE)
# -----------------------------------------------------------------------------
MYSQL_ROOT_PASSWORD=changeme_root_password
MYSQL_DATABASE=paris_immobilisations_db
MYSQL_USER=admin
MYSQL_PASSWORD=changeme_user_password
MYSQL_HOST=mysql
MYSQL_PORT=3306

# -----------------------------------------------------------------------------
# External API Configuration
# -----------------------------------------------------------------------------
# URL de l'API OpenData Paris
DATASET_API_URL=https://opendata.paris.fr/api/records/1.0/search
DATASET_ID=immobilisations-etat-des-amortissements
SEARCH_URL=https://opendata.paris.fr/api/records/1.0/search

# -----------------------------------------------------------------------------
# ETL Configuration
# -----------------------------------------------------------------------------
ETL_TABLE=immobilisations_amortissements
ETL_PAGE_SIZE=500
EXTRACTION_BATCH_SIZE=1000

# -----------------------------------------------------------------------------
# Logging Configuration
# -----------------------------------------------------------------------------
LOG_LEVEL=INFO

# -----------------------------------------------------------------------------
# Superset Configuration (OBLIGATOIRE)
# -----------------------------------------------------------------------------
# Crédentials admin Superset
SUPERSET_ADMIN_USER=admin
SUPERSET_ADMIN_PASSWORD=changeme_superset_password

# Clé secrète pour Superset (DOIT Être changée en production)
# Générer avec: openssl rand -base64 42
SUPERSET_SECRET_KEY=CHANGE_THIS_TO_A_LONG_RANDOM_SECRET_KEY_IN_PRODUCTION

# Port Superset
SUPERSET_WEBSERVER_PORT=8088
